unit PostoABC.Model.DAO.Contract;

interface

uses
  Data.DB;

type
  IModelDAOGeneric<T> = interface
    ['{6CBA96DC-B358-4D6F-8149-3E17D23053E4}']
    function Delete                        : IModelDAOGeneric<T>;
    function DataSet(AValue : TDataSource) : IModelDAOGeneric<T>;
    function Get                           : IModelDAOGeneric<T>;
    function Insert                        : IModelDAOGeneric<T>;
    function Update                        : IModelDAOGeneric<T>;
    function This                          : T;
  end;

implementation

end.
